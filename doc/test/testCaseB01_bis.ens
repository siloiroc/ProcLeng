; Inicio del programa
				RES 10
				MOVE #65533, .SP
				MOVE #65533, .IX
				BR /Main				;Instruccion BR, salto incondicional
Main:
				WRSTR /string1				;Instruccion Put_Line
				WRCHAR #10
				WRCHAR #13
				MOVE #456, #-2[.IX]			;Instruccion Move
				MOVE #-2[.IX], /0			;Instruccion Assign
				HALT ; Fin del programa
; Cadenas de texto
string1:		DATA "REGISTROS"
END ;Macroinstrucción END. Ensamblador da por finalizado el código.
