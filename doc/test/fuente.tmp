; Inicio del programa
RES 1
MOVE #65530, .SP
MOVE #65530, .IX
MOVE #4, #-2[.IX]			;Instruccion Move
MOVE #2, #-3[.IX]			;Instruccion Move
ADD #-2[.IX], #-3[.IX]
MOVE .A, #-4[.IX]
MOVE #-4[.IX], /0			;Instruccion Assign
MOVE /0, #-5[.IX]			;Instruccion Move
WRINT #-5[.IX]				;Instruccion Put_Line(expresión)
WRCHAR #10
WRCHAR #13
HALT ; Fin del programa
END ;Macroinstrucción END. Ensamblador da por finalizado el código.

